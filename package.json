{
  "name": "electoral-roll-processor",
  "version": "1.0.0",
  "description": "Process electoral roll PDFs with OCR and translation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node setup.js",
    "test": "node test/test.js",
    "docker:build": "docker build -t electoral-processor .",
    "docker:run": "docker run -p 3000:3000 electoral-processor",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "clean": "rm -rf uploads/* output/* temp/* logs/*",
    "logs": "tail -f logs/app-*.log"
  },
  "keywords": [
    "ocr",
    "tesseract",
    "pdf",
    "electoral-roll"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "fs-extra": "^11.3.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "sharp": "^0.33.5",
    "tesseract.js": "^6.0.1",
    "translate": "^2.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
